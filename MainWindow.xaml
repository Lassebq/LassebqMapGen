<Window x:Class="LassebqMapGen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LassebqMapGen"
        mc:Ignorable="d"
        Title="Lassebq's Map Generator" Height="160" Width="400">
    <Grid>
        <Button x:Name="button1" Content="&lt;Not Selected&gt;" Margin="127,14,10,0" VerticalAlignment="Top" Click="Button_Click_1"/>
        <Label Content="Selected World:" HorizontalAlignment="Left" Margin="10,14,0,0" VerticalAlignment="Top" Width="112"/>
        <Label Content="Preview Save Path:" HorizontalAlignment="Left" Margin="10,45,0,0" VerticalAlignment="Top" Width="112"/>
        <Button x:Name="button2" Content="&lt;Not Selected&gt;" Margin="127,47,10,0" VerticalAlignment="Top" Click="Button_Click_2"/>
        <Button x:Name="button3" Content="Generate Map Preview" Margin="0,0,10,10" Height="22" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="132" Click="Button_Click"/>

    </Grid>
</Window>
